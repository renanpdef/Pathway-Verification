  <element xsi:type="pathwayMetamodel:Prescription" id="32244" type="Receita" type_verbose="Receita" url="" name="[Grupo de Risco] Receita" description="&lt;p>&lt;strong>&lt;span style=&quot;font-size:18px&quot;>Checar hemograma completo para diagn&amp;oacute;stico diferencial com dengue.&lt;/span>&lt;/strong>&lt;/p>&#xA;&#xA;&lt;ul>&#xA;&#x9;&lt;li>&lt;span style=&quot;font-size:18px&quot;>Sugerir avalia&amp;ccedil;&amp;atilde;o ambulatorial com reumatologista se paciente permanecer com dor ap&amp;oacute;s a melhora dos sintomas agudos.&amp;nbsp;&lt;/span>&lt;/li>&#xA;&lt;/ul>" isTerminal="true" inputSequences="//@sequence.4">
