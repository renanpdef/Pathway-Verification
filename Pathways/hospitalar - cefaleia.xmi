  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="33381" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="Avaliar Sintomas/Sinais de Gravidade" description="" isInitial="true" inputSequences="//@sequence.10" outputSequences="//@sequence.0 //@sequence.4 //@sequence.9">
    <questions text="H&#xe1; sintomas/sinais de gravidade?" url="" id="15741">
      <answers xsi:type="pathwayMetamodel:Choice" name="sintomas" id="32121" weight="1" value="0.0">
        <option description="Neoplasia, imunossupress&#xe3;o, plaquetopenia, trombofilia, hipertens&#xe3;o grave" weight="1"/>
        <option description="Mudan&#xe7;a de padr&#xe3;o da cefal&#xe9;ia habitual (intensidade, frequ&#xea;ncia, dura&#xe7;&#xe3;o) ou pior cefaleia da vida ou in&#xed;cio ap&#xf3;s os 50 anos ou cefaleia que acorda o paciente ou aura maior que 1 hora, dor cervical ou ap&#xf3;s esfor&#xe7;o f&#xed;sico" weight="1"/>
        <option description="Suspeita de meningite (febre, v&#xf4;mitos, rigidez de nuca, pet&#xe9;quias)" weight="1"/>
        <option description="TCE moderado/grave ou TCE em idoso" weight="1"/>
        <option description="D&#xe9;ficit neurol&#xf3;gico focal, sinais de irrita&#xe7;&#xe3;o men&#xed;ngea (sinal kernig, lasague, brudzinski), s&#xed;ncope, convuls&#xe3;o, v&#xf4;mitos incoerc&#xed;veis, papiledema e rebaixamento do n&#xed;vel de consci&#xea;ncia" weight="1"/>
        <option description="Cefal&#xe9;ia p&#xf3;s-raquianest&#xe9;sica" weight="20"/>
      </answers>
    </questions>
    <questions text="Cefaleia Prim&#xe1;ria?" url="" id="15742">
      <answers xsi:type="pathwayMetamodel:Choice" name="cefaleia_primaria" id="32122" weight="1" value="0.0">
        <option description="Sim" weight="1"/>
        <option description="N&#xe3;o" weight="2"/>
      </answers>
    </questions>
  </element>
  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="33382" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="[Cefaleia Primaria] Classificar Cefaleia" description="Classificar tipo de cefaleia conforme sintomas apresentados" inputSequences="//@sequence.0" outputSequences="//@sequence.1 //@sequence.3 //@sequence.5 //@sequence.7">
    <questions text="Enxaqueca: Cefaleia de forte intensidade, latejante/puls&#xe1;til, piorando com as atividades do dia a dia. Dura de 4 a 72 horas. Geralmente a dor &#xe9; unilateral. Sintomas associados: n&#xe1;useas e/ou v&#xf4;mitos, foto e fonofobia" url="" id="15743">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="cefaleia_primaria_enxaqueca" id="32123" weight="1" value="false"/>
    </questions>
    <questions text="Cefaleia Tensional: Sensa&#xe7;&#xe3;o de aperto ou press&#xe3;o e, geralmente bilateral. Pode ser frontal, occipital ou holocraniana. Dor pode melhorar com atividades f&#xed;sicas. Costuma surgir no final da tarde. Relacionada com estresse f&#xed;sico, muscular ou emocional" url="" id="15744">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="cefaleia_primaria_tensional" id="32124" weight="1" value="false"/>
    </questions>
    <questions text="Cefaleia em Salvas: Unilateral, sempre do mesmo lado, de forte intensidade (excruciante), localizando-se em regi&#xe3;o orbital, supraorbital e/ou temporal. Associa&#xe7;&#xe3;o a sintomas auton&#xf4;micos ipsilaterais e dura&#xe7;&#xe3;o de 15 a 180 minutos" url="" id="15745">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="cefaleia_primaira_em_salvas" id="32125" weight="1" value="false"/>
    </questions>
    <questions text="RETORNO CEFALEIA PRIM&#xc1;RIA: Paciente em retorno sem melhora da dor e necessita de reavalia&#xe7;&#xe3;o e complementa&#xe7;&#xe3;o analg&#xe9;sica?" url="" id="15746">
      <answers xsi:type="pathwayMetamodel:YesOrNo" name="retorno" id="32126" weight="1" value="false"/>
    </questions>
  </element>
