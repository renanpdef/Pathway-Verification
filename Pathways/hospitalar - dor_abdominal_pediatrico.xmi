  <element xsi:type="pathwayMetamodel:AuxiliaryConduct" id="31229" type="AuxilioConduta" type_verbose="Aux&#xed;lio Conduta" url="" name="[Sem irrita&#xe7;&#xe3;o peritoneal] Avaliar foco" description="" inputSequences="//@sequence.9" outputSequences="//@sequence.10 //@sequence.11 //@sequence.12 //@sequence.16 //@sequence.17 //@sequence.21 //@sequence.25 //@sequence.29 //@sequence.33 //@sequence.34 //@sequence.38 //@sequence.39 //@sequence.41 //@sequence.43">
    <questions text="Avaliar foco da dor:" url="" id="14965">
      <answers xsi:type="pathwayMetamodel:Choice" name="sem_irrit_avaliar_foco" id="30225" weight="1" value="0.0">
        <option description="Dor abdominal simples sem altera&#xe7;&#xe3;o no exame f&#xed;sico, sem comorbidades" weight="1"/>
        <option description="Apendicite = Dor FID ou Blumberg positivo" weight="2"/>
        <option description="Suspeita de pancreatite aguda = dor em faixa de abdome superior, com irradia&#xe7;&#xe3;o para dorso associado &#xe0; n&#xe1;useas ou v&#xf4;mitos" weight="3"/>
        <option description="Dor abdominal da diarreia" weight="4"/>
        <option description="Obstru&#xe7;&#xe3;o intestinal: dor em c&#xf3;lica, v&#xf4;mitos, obstipa&#xe7;&#xe3;o e interrup&#xe7;&#xe3;o de elimina&#xe7;&#xe3;o de gases e fezes" weight="5"/>
        <option description="Cistite: dis&#xfa;ria, polaci&#xfa;ria, urg&#xea;ncia miccional" weight="6"/>
        <option description="Dor p&#xf3;s-trauma (esportivo, viol&#xea;ncia, automobil&#xed;stico)" weight="7"/>
        <option description="Diagn&#xf3;stico inconclusivo ap&#xf3;s realiza&#xe7;&#xe3;o de exames" weight="8"/>
        <option description="H&#xe9;rnia umbilical/abdominal com sinais de encarceramento" weight="9"/>
        <option description="DRGE/dispepsia: dor epig&#xe1;strica" weight="10"/>
        <option description="Nefrolit&#xed;ase: dor intensa, s&#xfa;bita, localizada em laterais do abdome e dorso associada &#xe0; n&#xe1;useas e v&#xf4;mitos, mic&#xe7;&#xe3;o excessiva ou frequente. Pode ter hematuria associada ou n&#xe3;o." weight="11"/>
        <option description="Doen&#xe7;a inflamat&#xf3;ria intestinal: suspeita Crohn, Doen&#xe7;a cel&#xed;aca, intoler&#xe2;ncia lactose, etc." weight="12"/>
        <option description="Constipa&#xe7;&#xe3;o - Hist&#xf3;ria de mais de 5 dias sem evacuar" weight="13"/>
        <option description="C&#xf3;lica do rec&#xe9;m-nascido (dor abdominal em RN maior de 14 dias associado a choro inconsol&#xe1;vel, abdome levemente distendido, hipertimp&#xe2;nico, com piora geralmente no fim do dia e aus&#xea;ncia de febre)" weight="14"/>
      </answers>
    </questions>
  </element>
