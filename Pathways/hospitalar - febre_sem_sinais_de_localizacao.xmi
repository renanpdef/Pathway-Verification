  <element xsi:type="pathwayMetamodel:Treatment" id="34522" type="Tratamento" type_verbose="Tratamento" url="" name="ATB + Interna&#xe7;&#xe3;o" description="&lt;p dir=&quot;ltr&quot;>&lt;u>&lt;strong>Discutir com chefe de plant&amp;atilde;o a necessidade de interna&amp;ccedil;&amp;atilde;o.&lt;/strong>&lt;/u>&lt;/p>&#xA;&#xA;&lt;p>Prescrever antibi&amp;oacute;ticoterapia conforme tabela abaixo:&lt;/p>&#xA;&#xA;&lt;table align=&quot;center&quot; border=&quot;1&quot; cellpadding=&quot;1&quot;>&#xA;&#x9;&lt;thead>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;th scope=&quot;col&quot;>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>&lt;strong>IDADE&lt;/strong>&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/th>&#xA;&#x9;&#x9;&#x9;&lt;th scope=&quot;col&quot;>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>&lt;strong>ALTERA&amp;Ccedil;&amp;Atilde;O LABORATORIAL&lt;/strong>&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/th>&#xA;&#x9;&#x9;&#x9;&lt;th scope=&quot;col&quot;>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>&lt;strong>ANTIBIOTICOTERAPIA&lt;/strong>&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/th>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&lt;/thead>&#xA;&#x9;&lt;tbody>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Menores de 28 dias&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames s&amp;eacute;ricos e urin&amp;aacute;rios normais ou alterados&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Ampi + Genta (dose: conorme idade e peso do RN)&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Entre 28 dias e 2 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames s&amp;eacute;ricos e urin&amp;aacute;rios normais ou alterados&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Ceftriaxona 50-80mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Menores de 2 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames s&amp;eacute;ricos e urin&amp;aacute;rios normais ou alterados + L&amp;iacute;quor alterado&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Ampicilina 200mg/kg/dia e Ceftriaxona para 100mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Menores de 2 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Fator de risco de herpes&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Associar Aciclovir 300mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>2 a 3 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames s&amp;eacute;ricos e urin&amp;aacute;rios alterados e L&amp;iacute;quor normal&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Ceftriaxona 50-80mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Maiores de 2 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames s&amp;eacute;ricos e urin&amp;aacute;rios normais ou alterados + L&amp;iacute;quor alterado&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Ceftriaxona 100mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Maiores de 2 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames s&amp;eacute;ricos normais e urin&amp;aacute;rios alterados&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Oral: Acetil-cefuroxima 30mg/kg/dia ou Amoxicilina + Clavulanato 50mg/kg/dia&lt;/p>&#xA;&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Parental: Ceftriaxina 50-80mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Maiores de 2 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames normais&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Sem antibioticoterapia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&#x9;&lt;tr>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Maiores de 3 meses&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Exames urin&amp;aacute;rios normais e hemograma e PCR alterado&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&#x9;&lt;td>&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Oral: Amoxicilina + Clavulanato 50mg/kg/dia&lt;/p>&#xA;&#xA;&#x9;&#x9;&#x9;&lt;p dir=&quot;ltr&quot; style=&quot;text-align:center&quot;>Parental: Ceftriaxina 50-80mg/kg/dia&lt;/p>&#xA;&#x9;&#x9;&#x9;&lt;/td>&#xA;&#x9;&#x9;&lt;/tr>&#xA;&#x9;&lt;/tbody>&#xA;&lt;/table>" isTerminal="true" inputSequences="//@sequence.15"/>
