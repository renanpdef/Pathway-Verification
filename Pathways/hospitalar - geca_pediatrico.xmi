  <element xsi:type="pathwayMetamodel:Information" id="34490" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="" name="[Sem melhora no quadro] Conduta" description="&lt;p>&lt;span style=&quot;font-size:18px&quot;>Aumentar velocidade de infus&amp;atilde;o.&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:18px&quot;>Iniciar HV de Manuten&amp;ccedil;&amp;atilde;o (Novo Holliday - SG1:1 + KCl10%).&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:18px&quot;>Iniciar dieta oral assim que poss&amp;iacute;vel.&amp;nbsp;&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:18px&quot;>Prescrever sintom&amp;aacute;ticos.&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>&lt;span style=&quot;font-size:18px&quot;>Avaliar interna&amp;ccedil;&amp;atilde;o.&lt;/span>&lt;/strong>&lt;/p>" isTerminal="true" inputSequences="//@sequence.12"/>
