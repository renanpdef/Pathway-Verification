  <element xsi:type="pathwayMetamodel:Treatment" id="26249" type="Tratamento" type_verbose="Tratamento" url="" name="Exames (Neutropenia Febril)" description="&lt;p>&lt;span style=&quot;font-size:14px&quot;>&lt;span style=&quot;color:#ffffff&quot;>&lt;strong>&lt;u>&lt;span style=&quot;background-color:#e74c3c&quot;>Encaminhar ao chefe de equipe. &lt;/span>&lt;/u>&lt;/strong>&lt;/span>&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;span style=&quot;font-size:14px&quot;>&lt;span style=&quot;color:#ffffff&quot;>&lt;span style=&quot;background-color:#e74c3c&quot;>&lt;strong>PROCURAR POSSIVEL FOCO DE INFEC&amp;Ccedil;&amp;Atilde;O: PELE, ABDOME E PER&amp;Iacute;NEO (N&amp;Atilde;O REALIZAR TOQUE RETAL), PICS, ACESSOS CENTRAIS, MUCOSITES, GASTROENTERITES AGUDAS, INFEC&amp;Ccedil;&amp;Otilde;ES DO TRATO RESPIRAT&amp;Oacute;RIO SUPERIOR E INFERIOR.&lt;/strong>&lt;/span>&lt;/span>&lt;/span>&lt;/p>&#xA;&#xA;&lt;p>&lt;strong>&lt;span style=&quot;color:#3498db&quot;>Aviso: Solicitar hemocultura, cultura de outros s&amp;iacute;tios,&amp;nbsp;exames de imagem e pun&amp;ccedil;&amp;atilde;o (via conduta complementar) de acordo com sinais e sintomatologia. &lt;u>Hemocultura deve ser solicitada o quanto antes dentro do internamento hospitalar.&lt;/u>&lt;/span>&lt;/strong>&lt;/p>" inputSequences="//@sequence.10" outputSequences="//@sequence.0">
    <examination url="HEMOGRAMA COMPLETO COM REVISAO DE LAMINAS" exam_id="28040481" limb_side="" quantity="1" reason="Protocolo de Neutropenia Febril" clinical_indication="Protocolo de Neutropenia Febril"/>
    <examination url="CREATININA  (SANGUE)" exam_id="28010540" limb_side="" quantity="1" reason="Protocolo de Neutropenia Febril" clinical_indication="Protocolo de Neutropenia Febril"/>
    <examination url="BILIRRUBINA TOTAL E FRACOES" exam_id="28010299" limb_side="" quantity="1" reason="Protocolo de Neutropenia Febril" clinical_indication="Protocolo de Neutropenia Febril"/>
    <examination url="RX TORAX: P.A - LAT" exam_id="32050054" limb_side="" quantity="1" reason="Neutropenia Febril" clinical_indication="n/a"/>
    <examination url="SUMARIO DE URINA  (TIPO I)" exam_id="28130367" limb_side="" quantity="1" reason="Paciente com quadro de neutropenia febril" clinical_indication="n/a"/>
    <examination url="GASOMETRIA VENOSA" exam_id="28011759" limb_side="" quantity="1" reason="Paciente com quadro de neutropenia febril" clinical_indication="n/a"/>
  </element>
