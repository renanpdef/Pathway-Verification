<?xml version="1.0" encoding="UTF-8"?>
<protocolosv2:Protocol
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:protocolosv2="http://www.example.org/protocolosv2"
    xsi:schemaLocation="http://www.example.org/protocolosv2 protocolosv2.ecore">
  <elemento
      xsi:type="protocolosv2:AuxiliaryConduct"
      name="Varificar critérios para SEPSE [qSOFA]"
      description="Calcule o indice qSOFA para o paciente"
      outputSequences="//@sequence.0 //@sequence.1 //@sequence.2">
    <questions
        text="Escala de Glasgow (GCS) &lt;= 14?">
      <answers
          xsi:type="protocolosv2:YesOrNo"
          name="ver_sepse_glasgow"/>
    </questions>
    <questions
        text="FR >= 22?">
      <answers
          xsi:type="protocolosv2:YesOrNo"
          name="ver_sepse_fr"/>
    </questions>
    <questions
        text="PAS &lt;= 100?">
      <answers
          xsi:type="protocolosv2:YesOrNo"
          name="ver_sepse_pas"/>
    </questions>
  </elemento>
  <elemento
      xsi:type="protocolosv2:AuxiliaryConduct"
      name="Verificar critérios SIRS"
      description="Verifique os critérios de SIRS do paciente"
      inputSequences="//@sequence.2"
      outputSequences="//@sequence.3 //@sequence.4">
    <questions
        text="TAX &lt; 35ºc ou > 37,8ºc?">
      <answers
          xsi:type="protocolosv2:YesOrNo"
          name="ver_sepse_sirs_tax"/>
    </questions>
    <questions
        text="FR > 20 ipm?">
      <answers
          xsi:type="protocolosv2:YesOrNo"
          name="ver_sepse_sirs_fr"/>
    </questions>
    <questions
        text="FC > 90 bpm?">
      <answers
          xsi:type="protocolosv2:YesOrNo"
          name="ver_sepse_sirs_fc"/>
    </questions>
  </elemento>
  <elemento
      xsi:type="protocolosv2:Referral"
      name="Encaminhar para tratamento de SEPSE"
      description="Paciente preencheu critérios de gravidade segundo escala qSOFA. Encaminhar ao protocolo de tratamento de SEPSE para seguir com o tratamento do paciente."
      inputSequences="//@sequence.0"
      protocol="/"/>
  <elemento
      xsi:type="protocolosv2:Referral"
      name="Encaminhar para tratamento de SIRS"
      description="Paciente preencheu critérios de gravidade segundo escala SIRS. Encaminhar ao protocolo de tratamento de SIRS para seguir com o tratamento do paciente."
      inputSequences="//@sequence.1 //@sequence.3"
      protocol="/"/>
  <elemento
      xsi:type="protocolosv2:Referral"
      name="Seguir tratamento vigente"
      description="Paciente não apresentou índice de gravidade segundo critérios estudados neste momento. Dar continuidade ao protocolo vigente."
      inputSequences="//@sequence.4"
      protocol="/"/>
  <sequence
      inputStep="//@elemento.2"
      outputStep="//@elemento.0"
      name="s1">
    <operation
        name="qsofa_maior_igual_dois"
        Operator="equalOrGreater">
      <operand
          xsi:type="protocolosv2:Numeric"
          name="ver_sepse_soma_qsofa"/>
      <operand
          xsi:type="protocolosv2:Numeric"
          name=""
          value="2.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@elemento.3"
      outputStep="//@elemento.0"
      name="s2">
    <operation
        name="qsofa_igual_um"
        Operator="equal">
      <operand
          xsi:type="protocolosv2:Numeric"
          name="ver_sepse_soma_qsofa"/>
      <operand
          xsi:type="protocolosv2:Numeric"
          value="1.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@elemento.1"
      outputStep="//@elemento.0"
      name="s3">
    <operation
        name="qsofa_zero"
        Operator="smallerThan">
      <operand
          xsi:type="protocolosv2:Numeric"
          name="ver_sepse_soma_qsofa"/>
      <operand
          xsi:type="protocolosv2:Numeric"
          value="1.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@elemento.3"
      outputStep="//@elemento.1"
      name="s4">
    <operation
        name="criterios_sirs_maior_igual_dois"
        Operator="equalOrGreater">
      <operand
          xsi:type="protocolosv2:Numeric"
          name="ver_sepse_soma_sirs"/>
      <operand
          xsi:type="protocolosv2:Numeric"
          value="2.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@elemento.4"
      outputStep="//@elemento.1"
      name="s5">
    <operation
        name="criterios_sirs_menor_dois"
        Operator="smallerThan">
      <operand
          xsi:type="protocolosv2:Numeric"
          name="ver_sepse_soma_sirs"/>
      <operand
          xsi:type="protocolosv2:Numeric"
          value="2.0"/>
    </operation>
  </sequence>
</protocolosv2:Protocol>
